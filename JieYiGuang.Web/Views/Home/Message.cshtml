$().ready(function () {
    $("#profileForm").validate({
        rules:{
            Name:"required",
            email:{
                required:true,
                email:true
            },
            phone:{
                required:true,
                minlength:5
            }
        },
        submitHandler:function (form)
        {
            $(form).ajaxSubmit();
        }
    });
});
@if(!String.IsNullOrWhiteSpace(ViewBag.Message))
{
    @:alert("@(ViewBag.Message)");
}
</script>
}
