@using JieYiGuang.Model
@using System.Linq
@{
    Layout = "../Shared/_Layout.cshtml";
    ViewBag.Title = "福州致辉光电科技有限公司（Devotop Photonics,Inc.）";
    ViewBag.KeyWord = "Devotop,致辉,光电,光学,镜片,透镜,柱面镜,偏振,lens,cylindrical,激光,镜头,成像,波片,晶体";
    ViewBag.Description = "致辉光电用最大的热情提供优质的光学产品和专业的技术服务。我们的产品包括平面光学，球面和柱面光学，晶体，偏振光学，以及光学装配件。广泛运用于激光行业，工业自动化，高端仪器，科研军工，成像和机器视觉等行业。";

}
@section Head{
}
@model JieYiGuang.Model.T_Message

<div class="contant">
    <div class="top"> </div>
    <!--star banner-->
    @Html.Partial("~/Views/Shared/_Partial_Banner.cshtml")
    <!--end banner-->
    <div class="pubanner">
        <object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,19,0" height="160" width="961">
            <param name="movie" value="flash/banner6.swf">
            <param name="quality" value="high">
            <embed src="@Url.ThemeContent("~/Content/themes/" + ThemeManager.Style + "/images/banner6.swf")" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" height="160" width="961">
        </object>
    </div>
    <div class="mian_warp">
        <div class="pucont">
            <div class="puconttitle"><span class="left logo"><a href="/"></a></span> </div>
            <div class="both"> </div>
            <div class="pucontcont">
                <div class="left w200">
                    <img alt="" src="@Url.ThemeContent("~/Content/themes/" + ThemeManager.Style + "/images/contactlefttitle.jpg")">
                    <ul class="leftmenu">
                        <li class="li"><a href="@(Url.Action("Message", "Home"))">Messages</a> </li>
                        <li><a href="@(Url.Action("Contact", "Home"))">Contact us</a> </li>
                    </ul>
                    <div class="both2"> </div>
                    <div class="leftlink"> 
                    <a href="@(Url.Action("Product", "Home", new { Id = 2}))"><img alt="" src="@Url.ThemeContent("~/Content/themes/" + ThemeManager.Style + "/images/imgleftlink1.jpg")"></a> 
                    <a href="@(Url.Action("Message", "Home"))"><img alt="" src="@Url.ThemeContent("~/Content/themes/" + ThemeManager.Style + "/images/imgleftlink2.jpg")"></a> 
                    <a href="@(Url.Action("Faq", "Home"))"><img alt="" src="@Url.ThemeContent("~/Content/themes/" + ThemeManager.Style + "/images/imgleftlink3.jpg")"></a>
                    </div>
                </div>
                <div class="right w725">
                    <div class="left w543">
