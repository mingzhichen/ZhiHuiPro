                                      <p>&nbsp;</p>
                                    <form id="profileForm" type="actionForm" action="/Home/DoMessage" method="post" >
                                        <input type="hidden" name="c" value="add">
                                        <ul>
                                            <li>
                                                <span class="a">First Name:</span><span class="b">
                                                    <input id="co_name" class="required" maxlength="40" name="Name" size="20" type="text" tabindex="1" value="@(Model.Name)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Last name:</span><span class="b">
                                                    <input id="co_nickname" maxlength="40" name="NickName" size="20" type="text" tabindex="2" value="@(Model.NickName)" />
                                                </span><span class="c"><span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Title:</span><span class="b">
                                                    <input id="co_Title" maxlength="40" name="Title" size="20" type="text" tabindex="3" value="@(Model.Title)" />
                                                </span><span class="c"><span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Company:</span><span class="b">
                                                    <input id="co_Company" maxlength="40" name="Company" size="20" type="text" tabindex="4" value="@(Model.Company)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Department:</span><span class="b">
                                                    <input id="co_Department" maxlength="40" name="Department" size="20" type="text" tabindex="5" value="@(Model.Department)" />
                                                </span><span class="c"><span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Address:</span><span class="b">
                                                    <input maxlength="40" name="Address" size="20" type="text" tabindex="6" value="@(Model.Address)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">City:</span><span class="b">
                                                    <input id="co_City" maxlength="40" name="City" size="20" type="text" tabindex="7" value="@(Model.City)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">State(Province):</span><span class="b">
                                                    <input id="co_nickname" maxlength="40" name="Province" size="20" type="text" tabindex="8" value="@(Model.Province)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Post Code:</span><span class="b">
                                                    <input id="co_PostCode" maxlength="40" name="PostCode" size="20" type="text" tabindex="9" value="@(Model.PostCode)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Country:</span><span class="b">
                                                    <input id="co_Country" maxlength="40" name="Country" size="20" type="text" tabindex="10" value="@(Model.Country)" />
                                                </span><span class="c"><span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Tel:</span><span class="b">
                                                    <input id="phone" maxlength="14" name="Phone" type="text" class="required phone" tabindex="11" value="@(Model.Phone)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Fax:</span><span class="b">
                                                    <input  maxlength="50" name="Fax" type="text" tabindex="12" value="@(Model.Fax)" />
                                                </span><span class="c"><span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Email:</span><span class="b">
                                                    <input  maxlength="50" name="Email" type="text" class="required email" tabindex="13" value="@(Model.Email)" />
                                                </span><span class="c">*<span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <span class="a">Web Site:</span><span class="b">
                                                    <input  maxlength="50" name="WebSite" type="text" tabindex="14" value="@(Model.WebSite)" />
                                                </span><span class="c"><span class="Validform_checktip"></span></span>
                                            </li>
                                            <li style="height:165px;">
                                                <span class="a">Details :</span><span class="b">
                                                    <textarea name="Contents" tabindex="14">@(Model.Contents)</textarea>
                                                </span><span class="c"><span class="Validform_checktip"></span></span>
                                            </li>
                                            <li>
                                                <input class="asksub" type="submit" id="asm" border="0" value="Submit">
                                                <input type="reset" value="Reset" onclick="change()" class="asksub" style="margin-left:35px;">
                                            </li>
                                        </ul>
                                    </form>
                                </div>
                                <p><span style="font-size:larger">&nbsp;</span></p>
                                <table border="0" cellpadding="0" cellspacing="0" width="540">
                                    <tbody>
                                        <tr>
                                            <td><div class="print" style="border-top:#CCCCCC dotted 1px;"> <span class="left"> <a href="#">&nbsp;</a></span> <span class="right"> <a href="#"> <img alt="" src="@Url.ThemeContent("~/Content/themes/" + ThemeManager.Style + "/images/jt4.jpg")"> Top</a></span> </div></td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="both2"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="both"> </div>
            </div>
            <div class="both"> </div>
        </div>
    </div>
</div>

